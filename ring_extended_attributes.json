  "helper_functions": {
    "get_nested_value": "def get_nested(d, path, default=None):\n    keys = path.split('.')\n    for key in keys:\n        if isinstance(d, dict):\n            d = d.get(key)\n        else:\n            return default\n        if d is None:\n            return default\n    return d",
    
    "type_coercion": {
      "str_float": "float(value) if value else None",
      "str_int": "int(value) if value else None", 
      "int_bool": "bool(int(value)) if value is not None else None",
      "bool_or_null": "value if value is not None else None"
    }
  }
